{
  "cSpell.words": [
    "commitlint",
    "cqrs",
    "dtos",
    "headerapikey",
    "logform",
    "mikro",
    "nestjs",
    "precommit",
    "prepush",
    "ratchapol",
    "ratchapoltt",
    "sonarjs",
    "thongta",
    "uncolorize",
    "websockets"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "json",
    "json5",
    "jsonc",
    "markdown",
    "typescript"
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
